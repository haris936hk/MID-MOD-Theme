{
  "type": "footer",
  "name": "Footer",
  "sections": {
    "footer": {
      "type": "footer",
      "blocks": {
        "main_navigation": {
          "type": "group",
          "name": "Main Navigation",
          "settings": {
            "content_direction": "row",
            "vertical_on_mobile": true,
            "horizontal_alignment": "flex-start",
            "vertical_alignment": "flex-start",
            "gap": 80,
            "width": "full-width",
            "padding-block-start": 60,
            "padding-block-end": 60
          },
          "blocks": {
            "shop_menu": {
              "type": "group",
              "name": "SHOP",
              "settings": {
                "content_direction": "column",
                "horizontal_alignment_flex_direction_column": "flex-start",
                "vertical_alignment_flex_direction_column": "flex-start",
                "gap": 20,
                "width": "fit-content"
              },
              "blocks": {
                "shop_title": {
                  "type": "text",
                  "settings": {
                    "text": "<p>shop</p>",
                    "type_preset": "h6",
                    "case": "lowercase",
                    "font_size": "1rem",
                    "color": ""
                  }
                },
                "shop_links": {
                  "type": "menu",
                  "settings": {
                    "menu": "footer-shop",
                    "layout": "vertical",
                    "font_size": "0.875rem"
                  }
                }
              },
              "block_order": ["shop_title", "shop_links"]
            },
            "support_menu": {
              "type": "group",
              "name": "SUPPORT",
              "settings": {
                "content_direction": "column",
                "horizontal_alignment_flex_direction_column": "flex-start",
                "vertical_alignment_flex_direction_column": "flex-start",
                "gap": 20,
                "width": "fit-content"
              },
              "blocks": {
                "support_title": {
                  "type": "text",
                  "settings": {
                    "text": "<p>support</p>",
                    "type_preset": "h6",
                    "case": "lowercase",
                    "font_size": "1rem",
                    "color": ""
                  }
                },
                "support_links": {
                  "type": "menu",
                  "settings": {
                    "menu": "footer-support",
                    "layout": "vertical",
                    "font_size": "0.875rem"
                  }
                }
              },
              "block_order": ["support_title", "support_links"]
            },
            "about_section": {
              "type": "group",
              "name": "ABOUT",
              "settings": {
                "content_direction": "column",
                "horizontal_alignment_flex_direction_column": "flex-start",
                "vertical_alignment_flex_direction_column": "flex-start",
                "gap": 20,
                "width": "custom",
                "custom_width": 40
              },
              "blocks": {
                "about_title": {
                  "type": "text",
                  "settings": {
                    "text": "<p>about</p>",
                    "type_preset": "h6",
                    "case": "lowercase",
                    "font_size": "1rem",
                    "color": ""
                  }
                },
                "about_text": {
                  "type": "text",
                  "settings": {
                    "text": "<p>jordan olson, give me paragraph for this <a href='#'>read more</a></p>",
                    "type_preset": "paragraph",
                    "font_size": "0.875rem",
                    "color": ""
                  }
                }
              },
              "block_order": ["about_title", "about_text"]
            }
          },
          "block_order": ["shop_menu", "support_menu", "about_section"]
        },
        "bottom_section": {
          "type": "group",
          "name": "Social Icons",
          "settings": {
            "content_direction": "row",
            "vertical_on_mobile": true,
            "horizontal_alignment": "space-between",
            "vertical_alignment": "center",
            "gap": 0,
            "width": "custom",
            "custom_width": 30,
            "padding-block-start": 40,
            "padding-block-end": 0,
            "margin-left": "auto"
          },
          "blocks": {
            "facebook_icon": {
              "type": "icon",
              "name": "Facebook",
              "settings": {
                "icon": "facebook",
                "link": "https://www.facebook.com/",
                "size": "medium",
                "color": "",
                "background": false,
                "border": false
              }
            },
            "instagram_icon": {
              "type": "icon",
              "name": "Instagram",
              "settings": {
                "icon": "instagram",
                "link": "https://www.instagram.com/",
                "size": "medium",
                "color": "",
                "background": false,
                "border": false
              }
            },
            "pinterest_icon": {
              "type": "icon",
              "name": "Pinterest",
              "settings": {
                "icon": "pinterest",
                "link": "https://www.pinterest.com/",
                "size": "medium",
                "color": "",
                "background": false,
                "border": false
              }
            }
          },
          "block_order": ["facebook_icon", "instagram_icon", "pinterest_icon"]
        }
      },
      "block_order": ["main_navigation", "bottom_section"],
      "settings": {
        "content_direction": "column",
        "vertical_on_mobile": true,
        "horizontal_alignment": "space-between",
        "vertical_alignment": "flex-start",
        "align_baseline": false,
        "horizontal_alignment_flex_direction_column": "space-between",
        "vertical_alignment_flex_direction_column": "flex-start",
        "gap": 0,
        "section_width": "page-width",
        "section_height": "",
        "color_scheme": "scheme-1",
        "background_media": "none",
        "video_position": "cover",
        "background_image_position": "cover",
        "padding-block-start": 60,
        "padding-block-end": 30
      }
    }
  },
  "order": ["footer"]
}